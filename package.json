{
  "name": "auth_service_vi",
  "version": "1.0.0",
  "description": "The authentication service for Edustripe.com",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && babel index.js -d dist && babel app -d dist/app",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo:all",
    "db:refresh": "npm run db:migrate:undo && npm run db:migrate && npm run db:seed",
    "db:seed": "sequelize db:seed:all",
    "start": "node dist/index.js",
    "lint": "eslint . --fix",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node index.js",
    "test": "cross-env NODE_ENV=test npm run db:refresh && cross-env NODE_ENV=test nyc mocha --require @babel/register --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edustripetech/auth_service_v1.git"
  },
  "author": "Edustripe Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/edustripetech/auth_service_v1/issues"
  },
  "homepage": "https://github.com/edustripetech/auth_service_v1#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.0.0",
    "redis": "^3.0.2",
    "sequelize": "^5.21.5",
    "uuid": "^7.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "sequelize-cli": "^5.5.1"
  }
}
